<% if @clinte_proveedor.errors.blank? %>
  $('#contact-modal').modal('hide');
  $('table tbody').append('<%= j render "user_row", clinte_proveedor: @clinte_proveedor %>');
 $('.alert').remove();
<% else %>
$(".alert").css("display", "block");
  // TODO: show the errors in the UI
  $('.alert').empty('<p></p>');
  <% @clinte_proveedor.errors.full_messages.each do |msg| %>
          $('.alert').append('<p><%=msg%></p>');
        <% end %>
 
<% end %>